    <members>
        <member name="M:InvoiceManagerApi.Controllers.CustomerController.GetAll">
            <summary>
            Retrieves all customers.
            </summary>
            <remarks>
            Returns a list of all customers that are not soft-deleted.
            </remarks>
            <returns>
            A list of customers.
            </returns>
            <response code="200">Customers were successfully retrieved.</response>
        </member>
        <member name="M:InvoiceManagerApi.Controllers.CustomerController.GetById(System.Int32)">
            <summary>
            Retrieves a customer by its identifier.
            </summary>
            <param name="id">The unique identifier of the customer.</param>
            <returns>
            The requested customer.
            </returns>
            <response code="200">Customer was successfully retrieved.</response>
            <response code="404">Customer with the specified id was not found.</response>
        </member>
        <member name="M:InvoiceManagerApi.Controllers.CustomerController.Create(InvoiceManagerApi.DTOs.CustomerDTOs.CustomerCreateRequest)">
            <summary>
            Creates a new customer.
            </summary>
            <param name="request">Customer creation data.</param>
            <returns>
            The newly created customer.
            </returns>
            <response code="201">Customer was successfully created.</response>
            <response code="400">The request body is invalid.</response>
        </member>
        <member name="M:InvoiceManagerApi.Controllers.CustomerController.DeleteSoft(System.Int32)">
            <summary>
            Soft deletes a customer.
            </summary>
            <remarks>
            Marks the customer as deleted without removing it from the database.
            </remarks>
            <param name="id">The unique identifier of the customer.</param>
            <response code="200">Customer was successfully soft deleted.</response>
            <response code="404">Customer with the specified id was not found.</response>
        </member>
        <member name="M:InvoiceManagerApi.Controllers.CustomerController.DeleteHard(System.Int32)">
            <summary>
            Permanently deletes a customer.
            </summary>
            <remarks>
            Completely removes the customer from the database.
            This operation is irreversible.
            </remarks>
            <param name="id">The unique identifier of the customer.</param>
            <response code="200">Customer was successfully permanently deleted.</response>
            <response code="404">Customer with the specified id was not found.</response>
        </member>
        <member name="M:InvoiceManagerApi.Controllers.CustomerController.Update(System.Int32,InvoiceManagerApi.DTOs.CustomerDTOs.CustomerUpdateRequest)">
            <summary>
            Updates an existing customer.
            </summary>
            <param name="id">The unique identifier of the customer.</param>
            <param name="request">Updated customer data.</param>
            <returns>
            The updated customer.
            </returns>
            <response code="200">Customer was successfully updated.</response>
            <response code="404">Customer with the specified id was not found.</response>
        </member>
